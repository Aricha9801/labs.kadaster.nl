      "@type" : "@id",
      "@context": {
        "@base": "http://bag.basisregistraties.overheid.nl/bag/id/nummeraanduiding/",
        "identificatiecode": "@id",
        "bijbehorendeWoonplaats" : {
             "@type": "@id",
             "@context": {
                "@base" : "http://bag.basisregistraties.overheid.nl/bag/id/woonplaats/",
                 "identificatiecode": "@id"
             }
        }
      }
    },
    "onderwijsfunctie": "VerblijfsobjectOnderwijsfunctie",
    "openbareRuimte": {
      "@type" : "@id",
      "@context": {
        "@base": "http://bag.basisregistraties.overheid.nl/bag/id/openbare-ruimte/",
        "identificatiecode": "@id",
        "bijbehorendeWoonplaats" : {
            "@type": "@id",
            "@context": {
                "@base" : "http://bag.basisregistraties.overheid.nl/bag/id/woonplaats/",
                "identificatiecode": "@id"
            }
        }
      }
    },
    "overigegebruiksfunctie":"VerblijfsobjectOverigeGebruiksfunctie",
    "pand": {
      "@type": "@id",
      "@context": {
        "@base" : "http://bag.basisregistraties.overheid.nl/bag/id/pand/",
        "identificatiecode": "@id"
      }
    },
    "sportfunctie": "VerblijfsobjectSportfunctie",
