    "data": "@nest",
    "gebruiksdoel": "@type",
    "gezondheidszorgfunctie": "VerblijfsobjectGezondheidszorgfunctie",
    "industriefunctie" : "VerblijfsobjectIndustriefunctie",
    "kantoorfunctie": "VerblijfsobjectKantoorfunctie",
    "logiesfunctie": "VerblijfsobjectLogiesfunctie",
