  bgt:Pand
    a owl:Class;
    bgt:wet wet:artikel7;
    dqv:hasQualityAnnotation quality-annotation:pand;
    rdfs:isDefinedBy bgt:;
    rdfs:label "Pand"@nl;
    rdfs:seeAlso
      "https://docs.geostandaarden.nl/imgeo/catalogus/bgt/#pand"^^xsd:anyURI,
      "https://docs.geostandaarden.nl/imgeo/catalogus/bgt/#pand-0"^^xsd:anyURI,
