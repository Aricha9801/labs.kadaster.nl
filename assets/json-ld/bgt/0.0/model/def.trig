  bgt:Overbruggingsdeel
    a owl:Class;
    rdfs:label "Overbruggingsdeel"@nl;
    rdfs:seeAlso
      "https://docs.geostandaarden.nl/imgeo/catalogus/bgt/#overbruggingsdeel"^^xsd:anyURI,
