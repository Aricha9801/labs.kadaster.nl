  # Kwaliteit

  ldqd:precision
    a dqv:Dimension;
    rdfs:comment "De accuraatheid van het gegeven"@nl;
    rdfs:label "Accuraatheid"@nl.

  ldqd:recency
    a dqv:Dimension;
    rdfs:comment "De actualiteit van een gegeven.  Hoe up-to-date is het gegeven ten opzichte van mogelijke wijzigingen?"@nl;
    rdfs:label "Actualiteit"@nl.

  quality-annotation:pand
    a dqv:QualityAnnotation;
    dqv:inDimension
      ldqd:recency,
      quality-dimension:geografischePrecisie;
    oa:hasBody
      [ rdf:value "Actualiteit: 6 mndn, relatieve precisie: 30 cm, voor overig zie hoofdstuk 4 Gegevenscatalogus BGT."@nl ];
    #oa:hasTarget bgt:Pand; # IMPLICIT
    rdfs:comment "Actualiteit: 6 mndn, relatieve precisie: 30 cm, voor overig zie hoofdstuk 4 Gegevenscatalogus BGT."@nl;
    rdfs:label "Kwaliteitsannotatie voor BGT Pand"@nl.

  quality-annotation:wegdeel
    a dqv:QualityAnnotation;
    dqv:inDimension
      ldqd:recency,
      quality-dimension:geografischePrecisie;
    oa:hasBody
      [ rdf:value "Actualiteit: 6 mndn, relatieve precisie: 30 cm, voor overig zie hoofdstuk 4 Gegevenscatalogus BGT."@nl ];
    #oa:hasTarget bgt:Wegdeel; # IMPLICIT
    rdfs:comment "Actualiteit: 6 mndn, relatieve precisie: 30 cm, voor overig zie hoofdstuk 4 Gegevenscatalogus BGT."@nl;
    rdfs:label "Kwaliteitsannotatie voor BGT Wegdeel"@nl.

  quality-dimension:geografischePrecisie
    a dqv:Dimension;
    rdfs:comment "De geografische accuraatheid van een gegeven.  Met welke precisie worden geografische gegevens ingewonnen en ontsloten?"@nl;
    rdfs:label "Positionele nauwkeurigheid"@nl;
    skos:altLabel "Positionele nauwkeurigheid"@nl;
    skos:narrower ldqd:precision;
    skos:prefLabel "Relatieve precisie"@nl.

  # Status

  [ a owl:AllDifferentFrom;
    owl:distinctMembers
      ( status:bestaand
        status:historie
        status:plan ) ].

  status:bestaand
    a bgt:Status;
