      [ owl:oneOf
          ( status:plaatsAangewezen
            status:plaatsIngetrokken ) ];
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/domeinwaarden#86-statusplaats"^^xsd:anyURI;
    skos:definition "Een aanduiding van alle waarden die de status van een ligplaats en een standplaats kan aannemen."@nl;
    skos:scopeNote "Een statusaanduiding kent een limitatief karakter en kan uitsluitend een van de genoemde waarden aannemen."@nl.

  status:plaatsAangewezen
    a bag:StatusPlaats;
    skos:definition "Plaats is aangewezen door het bevoegd gezag of geconstateerd."@nl;
    skos:prefLabel "Plaats aangewezen"@nl.

  status:plaatsIngetrokken
    a bag:StatusPlaats;
    skos:definition "Plaats is ingetrokken door het bevoegd gezag."@nl;
    skos:prefLabel "Plaats ingetrokken"@nl.

  bag:StatusVerblijfsobject
    owl:equivalentClass
      [ owl:oneOf
          ( status:nietGerealiseerdVerblijfsobject
            status:verblijfsobjectBuitenGebruik
            status:verblijfsobjectGevormd
            status:verblijfsobjectInGebruik
            status:verblijfsobjectInGebruikNietIngemeten
            status:verblijfsobjectIngetrokken
            status:verblijfsobjectTenOnrechteOpgevoerd
            status:verbouwingVerblijfsobject ) ];
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/domeinwaarden#87-statusverblijfsobject"^^xsd:anyURI;
    skos:definition "Een aanduiding van alle waarden die de status van een verblijfsobject kan aannemen."@nl;
    skos:scopeNote "Een statusaanduiding kent een limitatief karakter en kan uitsluitend een van de genoemde waarden aannemen."@nl.

  status:nietGerealiseerdVerblijfsobject
    a bag:StatusVerblijfsobject;
    skos:definition "Een verblijfsobject in een pand waarvoor een bouwvergunning was verleend of dat gerealiseerd zou worden in een reeds bestaand pand, maar waarvan in een ambtelijke verklaring van een daartoe bevoegd ambtenaar is vastgesteld dat wordt afgezien van de bouw of waarvan de bouwvergunning is ingetrokken."@nl;
    skos:prefLabel "Niet gerealiseerd verblijfsobject"@nl.

  status:verblijfsobjectBuitenGebruik
    a bag:StatusVerblijfsobject;
    skos:definition "Een verblijfsobject dat onderdeel uitmaakt van een pand dat in dusdanige bouwkundige staat is dat niet te verwachten is dat het pand zal worden hersteld en weer in gebruik zal worden genomen."@nl;
    skos:prefLabel "Verblijfsobject buiten gebruik"@nl.

  status:verblijfsobjectGevormd
    a bag:StatusVerblijfsobject;
    skos:definition "Een nieuw verblijfsobject dat deel gaat uitmaken van een nog niet gebouwd pand waarvoor een bouwvergunning is verleend of dat wordt gerealiseerd in een reeds bestaand pand."@nl;
    skos:prefLabel "Verblijfsobject gevormd"@nl.

  status:verblijfsobjectInGebruik
    a bag:StatusVerblijfsobject;
    skos:definition """
<section lang="nl">
  <p>Een verblijfsobject dat feitelijk de status <a href="https://bag2.basisregistraties.overheid.nl/bag/id/status/verblijfsobjectInGebruikNietIngemeten">Verblijfsobject in gebruik (niet ingemeten)</a> had en waarvan de definitieve geometrie is ingemeten.</p>
</section>"""^^rdf:HTML;
    skos:prefLabel "Verblijfsobject in gebruik"@nl.

  status:verblijfsobjectInGebruikNietIngemeten
    a bag:StatusVerblijfsobject;
    skos:definition "Een verblijfsobject waarvan de aanvrager van de bouwvergunning heeft gemeld dat de bouw is voltooid of waarvan in een ambtelijke verklaring van een daartoe bevoegd ambtenaar is vastgesteld dat het verblijfsobject feitelijk in gebruik is genomen of dat het weliswaar (nog) niet in gebruik is genomen maar naar het oordeel van de gemeente wel gebruiksgereed is, dan wel een verblijfsobject waarvan de verbouw is voltooid, dan wel een verblijfsobject dat door een calamiteit gedeeltelijk is verdwenen maar naar verwachting zal worden hersteld en in gebruik genomen."@nl;
    skos:prefLabel "Verblijfsobject in gebruik (niet ingemeten)"@nl.

  status:verblijfsobjectIngetrokken
    a bag:StatusVerblijfsobject;
    skos:definition "Een verblijfsobject dat als zodanig opgehouden heeft te bestaan omdat de aanvrager van een sloopvergunning heeft gemeld dat de sloop is voltooid of in een ambtelijke verklaring van een daartoe bevoegd ambtenaar is vastgesteld dat het verblijfsobject als zodanig niet meer bestaat."@nl;
    skos:prefLabel "Verblijfsobject ingetrokken"@nl.

  status:verblijfsobjectTenOnrechteOpgevoerd
    a bag:StatusVerblijfsobject;
    skos:definition "Een verblijfsobject dat niet had mogen worden opgenomen in de BAG-registratie en vervolgens is beëindigd."@nl;
    skos:prefLabel "Verblijfsobject ten onrechte opgevoerd"@nl.

  status:verbouwingVerblijfsobject
    a bag:StatusVerblijfsobject;
    skos:definition "Een verblijfsobject waarvoor een vergunning tot verbouw is verleend en waarbij de verbouwing nog niet is voltooid."@nl;
    skos:prefLabel "Verbouwing verblijfsobject"@nl.

  bag:StatusWoonplaats
    a owl:Class;
    owl:equivalentClass
      [ owl:oneOf
          ( status:woonplaatsAangewezen
            status:woonplaatsIngetrokken ) ];
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/domeinwaarden#83-statuswoonplaats"^^xsd:anyURI;
    skos:definition "Een aanduiding van alle waarden die de status van een woonplaats kan aannemen."@nl;
    skos:prefLabel "Status woonplaats"@nl;
    skos:scopeNote "Een statusaanduiding kent een limitatief karakter en kan uitsluitend een van de genoemde waarden aannemen."@nl.

  status:woonplaatsAangewezen
    a bag:StatusWoonplaats;
    skos:definition "Woonplaats is aangewezen door het bevoegd gezag of geconstateerd."@nl;
    skos:prefLabel "woonplaats aangewezen"@nl.

  status:woonplaatsIngetrokken
    a bag:StatusWoonplaats;
    skos:definition "Woonplaats is ingetrokken door het bevoegd gezag."@nl;
    skos:prefLabel "woonplaats ingetrokken"@nl.

  bag:Verblijfsobject
    dct:source
      [ sdo:contentUrl "https://wetten.overheid.nl/BWBR0023466/2012-10-01/#Hoofdstuk1_Artikel1"^^xsd:anyURI;
        sdo:name "Artikel 1 Wet basisregistratie adressen en gebouwen"@nl ];
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/objecttypen#68-verblijfsobject"^^xsd:anyURI;
    rdfs:subClassOf nen3610:GeoObject;
    sdo:image image:verblijfsobject;
    skos:definition "Een Verblijfsobject is de kleinste binnen een of meer panden gelegen en voor woon-, bedrijfsmatige, of recreatieve doeleinden geschikte eenheid van gebruik die ontsloten wordt via een eigen afsluitbare toegang vanaf de openbare weg, een erf of een gedeelde verkeersruimte, onderwerp kan zijn van goederenrechtelijke rechtshandelingen en in functioneel opzicht zelfstandig is."@nl;
    skos:scopeNote
      # Populatie
      "De basisregistratie adressen en gebouwen omvat gegevens over alle verblijfsobjecten op het Nederlands grondgebied die voldoen aan bovenstaande objectdefinitie. Gegevens over een nieuw verblijfsobject worden in de basisregistratie adressen en gebouwen opgenomen bij verlening van de voor het realiseren van het verblijfsobject benodigde toestemming of het opmaken van een in artikel 10, onderdeel b van de wet genoemd document van constatering. De gegevens over verblijfsobjecten blijven in de basisregistratie adressen en gebouwen aanwezig, ook indien het verblijfsobject als zodanig niet meer aanwezig is. De basisregistratie adressen en gebouwen bevat in principe geen gegevens over verblijfsobjecten die op 1 juli 2009 niet meer als zodanig aanwezig waren."@nl.
  image:verblijfsobject
    a sdo:ImageObject;
    sdo:contentUrl "asset/verblijfsobject.svg"^^xsd:anyURI.

  bag:Woonplaats
    dct:source
      [ sdo:contentUrl "https://wetten.overheid.nl/BWBR0023466/2012-10-01/#Hoofdstuk1_Artikel1"^^xsd:anyURI;
        sdo:name "Artikel 1 Wet basisregistratie adressen en gebouwen"@nl ];
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/objecttypen#61-woonplaats"^^xsd:anyURI;
    rdfs:subClassOf nen3610:GeoObject;
    skos:definition "Een woonplaats is een door het bevoegde gemeentelijke orgaan als zodanig aangewezen en van een naam voorzien gedeelte van het grondgebied van de gemeente."@nl;
    skos:prefLabel "Woonplaats"@nl;
    skos:scopeNote
      # Populatie
      "De basisregistratie adressen en gebouwen bevat alle officieel als zodanig vastgestelde woonplaatsen op het Nederlandse grondgebied. Nieuwe woonplaatsen worden in de basisregistratie adressen en gebouwen opgenomen op basis van daartoe aangewezen brondocumenten waarin het ontstaan van de woonplaats als zodanig is vastgelegd. Daarnaast bevat de basisregistratie adressen en gebouwen alle geconstateerde woonplaatsen in de zin van artikel 2, tweede lid, onderdeel b, sub 3 van de wet. De gegevens over woonplaatsen blijven in de basisregistratie aanwezig, ook indien de woonplaats als zodanig wordt opgeheven. De basisregistratie bevat in principe geen gegevens over woonplaatsen die voor 1 juli 2009 zijn opgeheven."@nl,
      # Kwaliteit
      "Een Woonplaats bestaat als hiertoe door het bevoegde gemeentelijke orgaan als zodanig een besluit is genomen."@nl,
      # Toelichting
      "De indeling in woonplaatsen volgt uit de formele aanwijzing van woonplaatsen door het bevoegde gemeentelijke orgaan."@nl.

  bag:bouwjaar
    a owl:DatatypeProperty;
    rdfs:domain bag:Pand;
    rdfs:range xsd:gYear;
    rdfs:seeAlso "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#743-oorspronkelijk-bouwjaar"^^xsd:anyURI;
    skos:definition
      [ dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
        rdf:value "De aanduiding van het jaar waarin een pand oorspronkelijk als bouwkundig gereed is of zal worden opgeleverd."@nl ];
    skos:prefLabel "heeft oorspronkelijk bouwjaar"@nl;
    skos:scopeNote "Bij het initieel opvoeren van een PAND wordt door de gemeente een reële inschatting gemaakt van het waarschijnlijke bouwjaar van het PAND. Bij het bouwkundig gereed opleveren van het PAND wordt deze waarde in voorkomende gevallen aangepast. Indien in latere jaren wijzigingen aan een pand worden aangebracht, leidt dit niet tot wijziging van het bouwjaar."@nl.

  bag:documentdatum
    a owl:DatatypeProperty;
    rdfs:domain bag:Pand;
    rdfs:range xsd:date;
    rdfs:seeAlso
      "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#716-documentdatum"^^xsd:anyURI,
      "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#726-documentdatum"^^xsd:anyURI,
      "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#739-documentdatum"^^xsd:anyURI,
      "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#746-documentdatum"^^xsd:anyURI,
      "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#765-documentdatum"^^xsd:anyURI,
      "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#775-documentdatum"^^xsd:anyURI,
      "https://imbag.github.io/catalogus/hoofdstukken/attributen--relaties#787-documentdatum"^^xsd:anyURI;
    skos:definition
      [ dct:source "Grondslagen Basis Gebouwen Registratie versie 2.3"@nl;
        rdf:value "De datum waarop het brondocument is vastgesteld, op basis waarvan een opname, mutatie of een verwijdering van gegevens ten aanzien van een pand heeft plaatsgevonden."@nl ];
    skos:prefLabel "heeft document datum"@nl;
    skos:scopeNote "Alleen een datum die gelijk is aan of die is gelegen voor de datum van het opnemen of muteren van het betreffende gegeven kan in de registratie worden opgenomen. In het geval het brondocument een besluit van een bestuursorgaan betreft, dan is de datum van het brondocument de datum waarop het besluit is genomen."@nl.

  bag:documentnummer
